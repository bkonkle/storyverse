{
  "name": "@bkonkle/storyverse-api",
  "version": "1.0.0",
  "description": "The Storyverse api server",
  "repository": {
    "type": "git",
    "url": "git@github.com:bkonkle/storyverse.git"
  },
  "author": "Brandon Konkle <brandon@konkle.us> (http://konkle.us)",
  "license": "MIT",
  "scripts": {
    "build": "tsc --incremental -p tsconfig.build.json",
    "build.watch": "tsc --watch --incremental -p tsconfig.build.json",
    "check.types": "tsc --pretty --noEmit",
    "db.backup": "ts-node --transpile-only src/scripts/db/backup.ts",
    "db.create": "ts-node --transpile-only src/scripts/db/create.ts",
    "db.drop": "ts-node --transpile-only src/scripts/db/drop.ts",
    "db.functions": "ts-node --transpile-only src/scripts/db/functions.ts",
    "db.migrate": "yarn typeorm migration:run",
    "db.reset": "yarn db.drop && yarn db.create && yarn db.migrate",
    "db.restore": "ts-node --transpile-only src/scripts/db/restore.ts",
    "db.tunnel": "ts-node --transpile-only src/scripts/db/tunnel.ts",
    "db.revert": "yarn typeorm migration:revert",
    "generate.schema": "graphql-codegen && prettier --write src/Schema.ts",
    "lint": "yarn eslint 'src/**/*{.js,.ts}'",
    "lint.fix": "yarn lint --fix",
    "start": "ts-node --transpile-only --files src/App.ts",
    "start.dev": "concurrently --handle-input \"wait-on dist/App.js && nodemon\" \"tsc --watch --incremental -p tsconfig.build.json\" ",
    "start.debug": "TS_NODE_FILES=true nodemon --config nodemon-debug.json",
    "start.prod": "node dist/Main.js",
    "test": "NODE_ENV=test jest",
    "test.cov": "NODE_ENV=test jest --coverage",
    "test.int": "NODE_ENV=test dotenv -- jest -i -c ./jest.integration.config.js --forceExit",
    "test.watch": "yarn test --watch",
    "typeorm": "ts-node --transpile-only --files node_modules/.bin/typeorm --config src/config/Database.ts"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.9",
    "@nestjs/core": "^7.6.9",
    "@nestjs/graphql": "^7.9.8",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.9",
    "@nestjs/schedule": "^0.4.2",
    "@nestjs/typeorm": "^7.1.5",
    "apollo-server-core": "^2.19.2",
    "apollo-server-express": "^2.19.2",
    "chalk": "^4.1.0",
    "change-case": "^4.1.2",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.13.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^14.2.1",
    "graphql-extensions": "^0.12.8",
    "graphql-tag": "^2.11.0",
    "graphql-type-datetime": "^0.2.4",
    "graphql-type-json": "^0.3.2",
    "graphql-type-uuid": "^0.2.0",
    "jwks-rsa": "^1.12.2",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.5.1",
    "rxjs": "^6.6.3",
    "ts-parse-database-url": "^1.0.3",
    "typeorm": "^0.2.30",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.6.9",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.1.6",
    "@types/graphql-type-uuid": "^0.2.3",
    "@types/morgan": "^1.9.2",
    "@types/passport-jwt": "^3.0.4",
    "@types/recursive-readdir": "^2.2.0",
    "@types/supertest": "^2.0.10",
    "@types/yargs": "^16.0.0",
    "@types/yup": "^0.29.11",
    "axios": "^0.21.1",
    "concurrently": "^5.2.0",
    "faker": "^5.3.1",
    "nodemon": "^2.0.7",
    "recursive-readdir": "^2.2.2",
    "supertest": "^6.1.3",
    "wait-on": "^5.2.1"
  },
  "resolutions": {
    "graphql": "^14.2.1"
  }
}

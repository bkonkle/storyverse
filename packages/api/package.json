{
  "name": "@bkonkle/storyverse-api",
  "version": "1.0.0",
  "description": "The Storyverse api server",
  "repository": {
    "type": "git",
    "url": "git@github.com:bkonkle/storyverse.git"
  },
  "author": "Brandon Konkle <brandon@konkle.us> (http://konkle.us)",
  "license": "MIT",
  "scripts": {
    "build.watch": "tsc --watch --incremental -p tsconfig.build.json",
    "build": "tsc --incremental -p tsconfig.build.json",
    "check.types": "tsc --pretty --noEmit",
    "db.generate": "prisma generate",
    "db.reset": "prisma migrate reset --preview-feature",
    "db.migrate": "prisma migrate dev --preview-feature",
    "db.migrate.deploy": "prisma migrate deploy --preview-feature",
    "gql.generate": "graphql-codegen",
    "lint": "yarn eslint 'src/**/*{.js,.ts}'",
    "lint.fix": "yarn lint --fix",
    "start": "node build/Server.js",
    "start.dev": "concurrently --handle-input \"wait-on build/Server.js && nodemon\" \"yarn build.watch\"",
    "test": "NODE_ENV=test jest",
    "test.cov": "NODE_ENV=test jest --coverage",
    "test.e2e": "NODE_ENV=test jest -i -c ./jest.e2e.config.js --forceExit",
    "test.watch": "yarn test --watch"
  },
  "dependencies": {
    "@prisma/client": "^2.18.0",
    "apollo-server-core": "^2.21.0",
    "apollo-server-express": "^2.21.0",
    "chalk": "^4.1.0",
    "date-fns": "^2.18.0",
    "date-fns-tz": "^1.1.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "graphql": "^15.5.0",
    "graphql-extensions": "^0.12.8",
    "graphql-tag": "^2.11.0",
    "graphql-type-json": "^0.3.2",
    "graphql-type-uuid": "^0.2.0",
    "jwks-rsa": "^1.12.3",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "ts-parse-database-url": "^1.0.3",
    "typescript": "^4.2.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/faker": "^5.1.7",
    "@types/graphql-type-uuid": "^0.2.3",
    "@types/morgan": "^1.9.2",
    "@types/recursive-readdir": "^2.2.0",
    "@types/supertest": "^2.0.10",
    "@types/yargs": "^16.0.0",
    "@types/yup": "^0.29.11",
    "concurrently": "^6.0.0",
    "dotenv-expand": "^5.1.0",
    "faker": "^5.4.0",
    "nock": "^13.0.10",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "prisma": "^2.18.0",
    "recursive-readdir": "^2.2.2",
    "supertest": "^6.1.3",
    "type-fest": "^0.21.2",
    "wait-on": "^5.2.1"
  },
  "resolutions": {
    "graphql": "^15.5.0"
  }
}

#!/usr/bin/env bash

echo $PWD

if [ -f "node_modules/immutable-tuple/dist/tuple.mjs" ];
  then rm node_modules/immutable-tuple/dist/tuple.mjs;
fi;

if [ -f "node_modules/graphql/index.mjs" ];
  then rm node_modules/graphql/index.mjs;
fi;

if [ -f "node_modules/iterall/index.mjs" ];
  then rm node_modules/iterall/index.mjs;
fi;

if [ -f "../../node_modules/immutable-tuple/dist/tuple.mjs" ];
  then rm ../../node_modules/immutable-tuple/dist/tuple.mjs;
fi;

if [ -f "../../node_modules/graphql/index.mjs" ];
  then rm ../../node_modules/graphql/index.mjs;
fi;

if [ -f "../../node_modules/iterall/index.mjs" ];
  then rm ../../node_modules/iterall/index.mjs;
fi;

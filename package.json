{
  "name": "@bkonkle/storyverse",
  "version": "1.0.0",
  "description": "The Storyverse monorepo package",
  "repository": {
    "type": "git",
    "url": "git@github.com:bkonkle/storyverse.git"
  },
  "author": "Brandon Konkle <brandon@konkle.us> (http://konkle.us)",
  "license": "MIT",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": [
      "**/*"
    ]
  },
  "scripts": {
    "build": "tsc --pretty --skipLibCheck",
    "check.types": "yarn build --noEmit",
    "format": "prettier --write",
    "format.all": "yarn format \"src/**/*.{js,jsx,ts,tsx}\"",
    "test": "NODE_ENV=test jest",
    "test.watch": "NODE_ENV=test jest --watch"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^24.0.19",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.11.6",
    "@types/ramda": "^0.26.33",
    "@types/react": "^16.9.11",
    "@types/react-native": "^0.60.22",
    "@types/recursive-readdir": "^2.2.0",
    "dotenv-cli": "^3.0.0",
    "graphql-tag": "^2.10.1",
    "husky": "^3.0.9",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "nodemon": "^1.19.4",
    "prettier": "^1.18.2",
    "recursive-readdir": "^2.2.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typescript": "3.6.4"
  },
  "resolutions": {
    "graphql": "14.5.8",
    "mem": "^4.0.0"
  }
}
